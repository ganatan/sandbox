{
  "name": "backend-javascript-cjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate-project-structure": "node tools/scripts/generate-project-structure.js",
    "dev": "cross-env NODE_ENV=development nodemon src/server",
    "dev:prod": "cross-env NODE_ENV=production nodemon src/server",
    "start": "cross-env NODE_ENV=development node src/server",
    "start:prod": "cross-env NODE_ENV=production node src/server",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coverage": "jest --coverage",
    "test:unit": "jest src/modules/**/__tests__/unit",
    "test:integration": "jest src/__tests__/integration",
    "test:e2e": "jest src/__tests__/e2e",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "serve": "cross-env NODE_ENV=production node dist/bundle.js",
    "serve:dev": "cross-env NODE_ENV=development node dist/bundle.js",
    "docker:remove-all-containers": "FOR /f %i IN ('docker ps -a -q') DO docker rm -f %i",
    "docker:remove-all-images": "FOR /f %i IN ('docker images -q') DO docker rmi -f %i",
    "docker:list-images": "docker images",
    "docker:list-containers": "docker ps -a",
    "docker:build": "docker build -t backend-javascript-cjs .",
    "docker:run": "docker run -d --name backend-javascript-cjs-container -p 3000:3000 backend-javascript-cjs",
    "docker:stop": "docker stop backend-javascript-cjs-container",
    "docker:remove-container": "docker rm backend-javascript-cjs-container",
    "docker:remove-image": "docker rmi backend-javascript-cjs",
    "docker:stop-remove": "docker stop backend-javascript-cjs-container && docker rm backend-javascript-cjs-container",
    "docker:force-remove": "docker rm -f backend-javascript-cjs-container",
    "docker:clean-all": "npm run docker:force-remove && npm run docker:remove-image",
    "compose:up": "docker-compose up -d",
    "compose:up:build": "docker-compose up -d --build",
    "compose:up:force": "docker-compose up -d --build --force-recreate",
    "compose:down": "docker-compose down",
    "compose:restart": "docker-compose down && docker-compose up -d",
    "compose:rebuild-force": "docker-compose down && docker-compose up -d --build --force-recreate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.5.0",
    "dotenv-webpack": "^8.1.0",
    "express": "5.1.0",
    "express-rate-limit": "7.5.0",
    "helmet": "8.1.0",
    "nodemon": "3.1.10",
    "supertest": "7.1.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "webpack-node-externals": "^3.0.0",
    "winston": "3.17.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "cross-env": "^7.0.3",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "webpack": "5.99.7",
    "webpack-cli": "6.0.1"
  }
}
