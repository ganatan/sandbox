{
  "name": "backend-javascript-esm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate-project-structure": "node tools/scripts/generate-project-structure.js",
    "dev": "nodemon src/server",
    "dev:prod": "cross-env NODE_ENV=production nodemon src/server",
    "start": "node src/server",
    "start:prod": "cross-env NODE_ENV=production node src/server",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:unit": "cross-env NODE_ENV=test jest src/modules --testPathPattern=__tests__/unit",
    "test:integration": "cross-env NODE_ENV=test jest src/__tests__/integration",
    "test:e2e": "cross-env NODE_ENV=test jest src/__tests__/e2e",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "serve": "node dist/bundle.js",
    "docker:remove-all-containers": "FOR /f %i IN ('docker ps -a -q') DO docker rm -f %i",
    "docker:remove-all-images": "FOR /f %i IN ('docker images -q') DO docker rmi -f %i",
    "docker:list-containers": "docker ps -a",
    "docker:list-images": "docker images",
    "docker:build": "docker build -t backend-javascript-esm .",
    "docker:remove-image": "docker rmi backend-javascript-esm",
    "docker:remove-container": "docker rm -f backend-javascript-esm-container",
    "docker:run": "npm run docker:remove-container && docker run -d --name backend-javascript-esm-container -p 8080:8080 backend-javascript-esm",
    "docker:stop": "docker stop backend-javascript-esm-container",
    "docker:logs": "docker logs backend-javascript-esm-container",
    "docker:stop-remove": "docker stop backend-javascript-esm-container && docker rm backend-javascript-esm-container",
    "docker:force-remove": "docker rm -f backend-javascript-esm-container",
    "docker:clean-all": "npm run docker:force-remove && npm run docker:remove-image",
    "compose:up": "docker-compose up -d",
    "compose:up:build": "docker-compose up -d --build",
    "compose:up:force": "docker-compose up -d --build --force-recreate",
    "compose:down": "docker-compose down",
    "compose:restart": "docker-compose down && docker-compose up -d",
    "compose:rebuild-force": "docker-compose down && docker-compose up -d --build --force-recreate"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@babel/preset-env": "7.26.9",
    "cors": "2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "5.1.0",
    "helmet": "8.1.0",
    "nodemon": "3.1.10",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "eslint": "9.25.1",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "supertest": "7.1.0",
    "webpack": "5.99.7",
    "webpack-cli": "6.0.1"
  }
}
